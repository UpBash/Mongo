docker exec -it your_mongodb_container_name mongo your_database_name --eval 'db.Kinematic_2019_may.aggregate([
  {
    $project: {
      _id: 1,
      vessel_id: 1,
      t: 1, 
      type: { $literal: "Point" },
      coordinates: [
        { $toDouble: "$lon" },
        { $toDouble: "$lat" }
      ]
    }
  },
  {
    $out: "Fir"
  }
])'
